    <p>function shellSort(arr) {
 var len = arr.length;
 for(var gap = Math.floor(len / 2); gap &gt; 0; gap = Math.floor(gap / 2)) {
     // æ³¨æ„ï¼šè¿™é‡Œå’ŒåŠ¨å›¾æ¼”ç¤ºçš„ä¸ä¸€æ ·ï¼ŒåŠ¨å›¾æ˜¯åˆ†ç»„æ‰§è¡Œï¼Œå®é™…æ“ä½œæ˜¯å¤šä¸ªåˆ†ç»„äº¤æ›¿æ‰§è¡Œ
     for(var i = gap; i &lt; len; i++) {
         var j = i;
         varcurrent = arr[i];
         while(j - gap &gt;= 0 &amp;&amp; current &lt; arr[j - gap]) {
              arr[j] = arr[j - gap];
              j = j - gap;
         }
         arr[j] = current;
     }
 }
 return arr;
}</p>
