function bubbleSort(arr) {
 let time=0
 var len = arr.length;
 for (var i = 0; i &lt; len - 1; i++) {
     for (var j = 0; j &lt; len - 1 - i; j++) {
         if (arr[j] &gt; arr[j + 1]) {// ç›¸é‚»å…ƒç´ ä¸¤ä¸¤å¯¹æ¯”
             var temp = arr[j + 1];// å…ƒç´ äº¤æ¢
             arr[j + 1] = arr[j];
             arr[j] = temp;
             time++;
             // console.log(j, arr[j])
             // console.log(j + 1, arr[j])
             console.log(time,â€™bubbleSort: â€˜, arr)
             console.log(â€˜â€”â€”â€”â€”â€”â€”-â€˜)
         }
     }
 }
