var len = arr.length,
    partitionIndex,
    left = typeofleft != 'number'? 0 : left,
    right = typeofright != 'number'? len - 1 : right;
 
if(left &lt; right) {
    partitionIndex = partition(arr, left, right);
    quickSort(arr, left, partitionIndex-1);
    quickSort(arr, partitionIndex+1, right);
}
return arr; }
