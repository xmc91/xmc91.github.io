<p>function partition(arr, left ,right) {     // åˆ†åŒºæ“ä½œ
    var pivot = left,                      // è®¾å®šåŸºå‡†å€¼ï¼ˆpivotï¼‰
        index = pivot + 1;
    for(var i = index; i &lt;= right; i++) {
        if(arr[i] &lt; arr[pivot]) {
            swap(arr, i, index);
            index++;
        }     <br />
    }
    swap(arr, pivot, index - 1);
    return index-1;
}</p>

<p>function swap(arr, i, j) {
    var temp = arr[i];
    arr[i] = arr[j];
    arr[j] = temp;
}</p>
